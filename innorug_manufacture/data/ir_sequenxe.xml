<?xml version="1.0" encoding="utf-8"?>
<odoo>
   <data>
   <record id="seq_job_work" model="ir.sequence">
       <field name="name">JOB WORK</field>
       <field name="code">mrp.job.work</field>
       <field name="prefix">JW/</field>
       <field name="padding">5</field>
       <field name="company_id" eval="False"/>
   </record>


   <record id="seq_cost_center" model="ir.sequence">
       <field name="name">Cost Center</field>
       <field name="code">mrp.cost.center</field>
       <field name="prefix">CC/%(range_year)s/</field>
       <field name="padding">5</field>
       <field name="company_id" eval="False"/>
   </record>
     <record id="main_job_work" model="ir.sequence">
       <field name="name">Main Job Work</field>
       <field name="code">main.jobwork</field>
       <field name="prefix">MJW/%(range_year)s/</field>
       <field name="padding">5</field>
       <field name="company_id" eval="False"/>
   </record>


    <record id="main_cost_center" model="ir.sequence">
       <field name="name">Main Cosr Center</field>
       <field name="code">main.costcenter</field>
       <field name="prefix">MCC/%(range_year)s/</field>
       <field name="padding">5</field>
       <field name="company_id" eval="False"/>
   </record>
</data>
</odoo> 